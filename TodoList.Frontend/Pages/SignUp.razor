@page "/SignUp"
@inherits SignUpBase
<h1>Login </h1>


<form @onsubmit="@(()=>OnSubmitClick())" class="col-lg-6">
    <div class="form-group">
        <label for="user name">User Name </label>
        <input type="text" @bind="request.UserName" class="form-control" />
    </div>

    <div class="form-group">
        <label for="Email1">Email </label>
        <input type="email" @bind="request.Email" class="form-control" />

    </div> <div class="form-group">
        <label for="password">Password </label>
        <input type="password" @bind="request.Password" class="form-control" />
    </div>

    <div class="form-group">
        <label for="password">Confirm Password </label>
        <input type="password" @bind="request.ConfirmPassword"class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
    <button type="button" class="btn btn-primary" @onclick="@(()=>navManager.NavigateTo("/"))">Go Back</button>
</form>